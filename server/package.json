{
  "name": "flixpi-server",
  "version": "1.0.0",
  "description": "FlixPi Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "cross-env NODE_ENV=development node -r ./load-env.js src/index.js",
    "dev:test": "cross-env NODE_ENV=development USE_TEST_DATA=true node -r ./load-env.js src/index.js",
    "dev:watch": "cross-env NODE_ENV=development nodemon -r ./load-env.js src/index.js",
    "prod": "cross-env NODE_ENV=production node -r ./load-env.js src/index.js",
    "prod:watch": "cross-env NODE_ENV=production nodemon -r ./load-env.js src/index.js"
  },
  "keywords": [
    "netflix",
    "streaming",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^131.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2"
  }
}
